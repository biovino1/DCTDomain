    "# Load pfam_nomax50_pairs.pkl\n",
    "import pickle\n",
    "\n",
    "with open('pfam_nomax50_pairs.pkl', 'rb') as f:\n",
    "    pairs = pickle.load(f)\n",
    "\n",
    "# Print pairs that have 1 match\n",
    "count = 0\n",
    "for pair, matches in pairs.items():\n",
    "    if matches[0] == 21:\n",
    "        count += 1\n",
    "\n",
    "print(count)"
   ]
  }
